cls

for /f "tokens=4 delims=. " %%: in ('ver') do if %%: GTR 5 (set "v=v4") else ("v=")
set "mon="
for /f "skip=2 delims=," %%: in ('typeperf "\UDP%v%\*" -si 0 -sc 1') do (
	if not defined mon (
		for /f "tokens=1-7 delims=.:/ " %%a in (%%:) do (
			set mon=%%a
			set date=%%b
			set year=%%c
			set hour=%%d
			set minute=%%e
			set sec=%%f
			set ms=%%g
		)
	)
)

